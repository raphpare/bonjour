{
    "name": "bonjour",
    "version": "0.0.0-beta.1",
    "description": "Week calendar",
    "keywords": [
        "calendar",
        "agenda",
        "day",
        "week",
        "month",
        "year"
    ],
    "license": "ISC",
    "author": "Raphaël Paré <raph.pare@outlook.com> (https://github.com/raphpare)",
    "files": [
        "lib"
    ],
    "main": "lib/index.js",
    "module": "lib/index.esm.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/raphpare/bonjour.git"
    },
    "scripts": {
        "lint": "eslint",
        "build": "rimraf lib && node esbuild.config.js && yarn lint",
        "watch": "rimraf lib && node esbuild.config.js --watch",
        "typedoc": "rimraf docs && typedoc --out docs ./src",
        "clean": "rimraf node_modules && rimraf lib && rimraf docs",
        "serve": "nodemon test/server.mjs",
        "open-browser": "node test/open-browser.mjs",
        "dev": "yarn open-browser && concurrently \"yarn run watch\" \"yarn run serve\""
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "autoprefixer": "^10.4.7",
        "concurrently": "^7.2.2",
        "esbuild": "^0.14.2",
        "esbuild-sass-plugin": "^2.2.6",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "express": "^4.18.1",
        "nodemon": "^2.0.20",
        "open": "^8.4.0",
        "postcss": "^8.4.14",
        "postcss-preset-env": "^7.7.1",
        "prettier": "^2.8.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.9.3"
    },
    "browserslist": {
        "browsers": "last 4 versions, > 1%"
    }
}
